container_commands:
    01-clear-cache:
        command: "php artisan optimize:clear"
        leader_only: true

    02-config-cache:
        command: "php artisan config:cache"
        leader_only: true

    03-route-cache:
        command: "php artisan route:cache"
        leader_only: true

    04-migration:
        command: "php artisan migrate"
        leader_only: true

    05-seeding:
        command: "php artisan db:seed"
        leader_only: true
